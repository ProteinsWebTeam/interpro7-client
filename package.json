{
  "name": "interpro7-client",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/interpro7-client.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "preanalyze": "npm run report:stats",
    "analyze": "npx webpack-bundle-analyzer reports/webpack-stats/stats.json",
    "test": "npm-run-all test:flow test:lint test:unit",
    "test:flow": "flow focus-check --merge-timeout=1000 .",
    "test:lint": "eslint --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" \"./**/*.js\"",
    "test:unit": "jest --notify src",
    "test:functional": "[[ -n $IS_EBI_CLUSTER ]] && echo 'skipped functionnal test on EBI cluster' || jest --notify tests",
    "report": "npm-run-all -s --continue-on-error clean:report report:**",
    "report:build": "node scripts/report-build-info.js",
    "report:coverage:flow": "flow-coverage-report",
    "report:coverage:jest": "npm run test:unit -- --coverage",
    "report:lighthouse": "node scripts/lighthouse.js",
    "report:stats": "mkdirp reports/webpack-stats && webpack --profile --json > reports/webpack-stats/stats.json",
    "coveralls": "cat reports/jest-coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --mode=development",
    "flow-typed:install": "npx flow-typed install",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "clean:report": "rimraf reports && mkdirp reports",
    "bundle": "webpack --progress",
    "postbundle": "npm run test:functional",
    "precommit": "lint-staged",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:dev": "npm run deploy -- -- --mode=development",
    "deploy:staging": "npm run deploy -- -- --env.staging",
    "deploy:production": "npm run deploy",
    "start": "npm run dev",
    "warmup": "node scripts/website-warmer.js"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": true,
          "targets": {
            "browsers": "> 0.25%",
            "node": true
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "dev": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "babel-plugin-dynamic-import-node"
        ]
      },
      "production": {
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "lint-staged": {
    "*.+(js|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js"
    ],
    "concurrentFiles": 4,
    "threshold": 50,
    "type": [
      "text",
      "html",
      "json"
    ],
    "outputDir": "reports/flow-coverage"
  },
  "lighthouse": {
    "threshold": 20
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.0.0",
    "EBI-Icon-fonts": "1.3.0-alpha.2",
    "babel-polyfill": "6.26.0",
    "clanviewer": "0.1.8",
    "color-hash": "1.0.3",
    "core-js": "2.5.7",
    "d3": "5.4.0",
    "data-loader": "0.3.3",
    "dompurify": "1.0.4",
    "draft-js": "0.10.5",
    "ebi-framework": "1.3.0",
    "filesize": "3.6.1",
    "foundation-sites": "^6.4.3",
    "gsap": "2.0.1",
    "history": "4.7.2",
    "idb": "2.1.1",
    "interpro-components": "0.6.1",
    "isomorphic-fetch": "2.2.1",
    "litemol": "github:dsehnal/LiteMol",
    "lodash-es": "4.17.10",
    "pdb-web-components": "^0.2.0",
    "popper.js": "1.14.3",
    "prop-types": "15.6.1",
    "protvista-interpro-track": "0.0.9",
    "protvista-manager": "0.0.9",
    "protvista-navigation": "0.0.7",
    "protvista-sequence": "0.0.6",
    "protvista-track": "0.0.9",
    "protvista-zoomable": "0.0.8",
    "react": "16.4.0",
    "react-addons-transition-group": "15.6.2",
    "react-dom": "16.4.0",
    "react-helmet": "5.2.0",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-tippy": "^1.2.2",
