{
  "name": "new_interpro_webviews",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/new_interpro_webviews.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:flow": "if flow version; then flow check; else echo \"flow unable to run, skipping\"; fi",
    "test:lint": "esw --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" \"./**/*.js\"",
    "test:unit": "NODE_ENV=test mocha-webpack --webpack-config webpack/config.js --growl --reporter dot \"src/**/*test?(s).js\"",
    "test:functional": "echo \"Warning, no functional test specified yet, passing\" && exit 0",
    "dev": "webpack-dev-server --config webpack/config.js",
    "dev:test": "npm-run-all -rp dev:test:*",
    "dev:test:lint": "npm run test:lint -- --watch",
    "dev:test:unit": "npm run test:unit -- --watch",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "bundle": "webpack --progress --config webpack/config.js",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:staging": "npm run deploy",
    "deploy:production": "NODE_ENV=production npm run deploy",
    "start": "npm-run-all -rp dev:test dev",
    "stats": "NODE_ENV=production webpack --config webpack/config.js --profile --json > .stats/stats.json"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": true,
          "targets": {
            "browsers": "last 1 version, > 10%",
            "node": true,
            "exclude": [
              "typed.data-view",
              "typed.int8-array",
              "typed.uint8-array",
              "typed.uint8-clamped-array",
              "typed.int16-array",
              "typed.uint16-array",
              "typed.int32-array",
              "typed.uint32-array",
              "typed.float32-array",
              "typed.float64-array"
            ]
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-react-constant-elements",
      "transform-react-jsx-source"
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ],
        "comments": false
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "color-hash": "^1.0.3",
    "core-js": "^2.4.1",
    "d3": "^4.8.0",
    "data-loader": "^0.1.8",
    "draft-js": "^0.10.1",
    "foundation-sites": "~6.2.4",
    "gsap": "^1.19.1",
    "history": "^4.6.2",
    "idb": "^2.0.3",
    "interpro-components": "^0.1.7",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.2",
    "pdb-web-components": "^0.1.3",
    "popper.js": "^1.10.1",
    "prop-types": "^15.5.10",
    "query-string": "^4.3.4",
    "react": "^15.6.1",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "reselect": "^3.0.1",
    "timeago.js": "^3.0.1",
    "timing-functions": "^0.1.2",
    "url": "^0.11.0",
    "webcomponents.js": "github:webcomponents/webcomponentsjs#v1.0.0-rc.10"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-babili": "^0.1.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "empty": "^0.10.1",
    "eslint": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.48.0",
    "flow-typed": "^2.1.2",
    "glob": "^7.1.2",
    "glob-loader": "^0.3.0",
    "html-webpack-plugin": "^2.28.0",
    "img-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "jsx-chai": "^4.0.0",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "style-loader": "^0.18.2",
    "uglify-js": "mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "why-did-you-update": "0.0.8",
    "yaml-loader": "^0.4.0"
  }
}
