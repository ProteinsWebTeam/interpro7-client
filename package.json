{
  "name": "new_interpro_webviews",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/new_interpro_webviews.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:flow": "flow check || echo \"flow unable to run, skipping\"",
    "test:lint": "esw --color --ignore-path .gitignore \"./**/*.js\"",
    "test:unit": "NODE_ENV=test mocha-webpack --webpack-config webpack/config.js --growl --reporter dot \"src/**/*test?(s).js\"",
    "test:functional": "echo \"Warning, no functional test specified yet, passing\" && exit 0",
    "dev": "webpack-dev-server --config webpack/config.js",
    "dev:test": "npm-run-all -rp dev:test:*",
    "dev:test:lint": "npm run test:lint -- --watch",
    "dev:test:unit": "npm run test:unit -- --watch",
    "lint:fix": "esw --color --ignore-path .gitignore \"./**/*.js\" --fix",
    "clean": "rimraf dist",
    "bundle": "webpack --progress --config webpack/config.js",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:staging": "npm run deploy",
    "deploy:production": "NODE_ENV=production npm run deploy",
    "start": "npm-run-all -rp dev:test dev",
    "stats": "webpack --profile --json > .stats/stats.json"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": "commonjs"
        }
      ],
      "es2016",
      "es2017",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-react-constant-elements",
      "transform-runtime",
      "transform-react-jsx-source"
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      }
    }
  },
  "dependencies": {
    "color-hash": "^1.0.3",
    "data-loader": "^0.1.6",
    "foundation-sites": "^6.2.4",
    "history": "^3.2.1",
    "interpro-components": "^0.1.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.16.6",
    "pdb-web-components": "^0.1.2",
    "react": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-babili": "0.0.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "empty": "^0.10.1",
    "eslint": "^3.9.1",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-react": "^6.6.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.34.0",
    "glob": "^7.1.0",
    "glob-loader": "^0.3.0",
    "html-webpack-plugin": "^2.24.1",
    "iflow-react-router": "^1.1.17",
    "img-loader": "^1.3.1",
    "json-loader": "^0.5.4",
    "jsx-chai": "^4.0.0",
    "mocha": "^3.1.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^3.11.2",
    "npm-run-all": "^3.1.0",
    "postcss-apply": "^0.4.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.3",
    "postcss-loader": "^1.1.0",
    "react-addons-test-utils": "^15.3.2",
    "redbox-react": "^1.3.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^2.0.0-pre",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url": "^0.11.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.1",
    "yaml-loader": "^0.4.0"
  }
}
