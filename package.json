{
  "name": "interpro7-client",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/interpro7-client.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "npm-run-all -p --aggregate-output test:flow test:lint test:unit",
    "test:flow": "flow focus-check --merge-timeout=1000 .",
    "test:lint": "esw --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" \"./**/*.js\"",
    "test:unit": "jest --notify src",
    "test:functional": "jest --notify tests",
    "report": "npm-run-all -s --continue-on-error clean:report report:*",
    "report:build": "node scripts/report-build-info.js",
    "report:coverage:flow": "flow-coverage-report",
    "report:coverage:jest": "jest src tests --runInBand --coverage",
    "report:lighthouse": "node scripts/lighthouse.js",
    "report:stats": "mkdirp reports/webpack-stats && webpack --env.production --profile --json > reports/webpack-stats/stats.json",
    "dev": "webpack-dev-server",
    "dev:test": "npm-run-all -rp dev:test:*",
    "dev:test:lint": "npm run test:lint -- --watch",
    "dev:test:unit": "npm run test:unit -- --watch",
    "flow-typed:install": "npx flow-typed install",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "clean:report": "rimraf reports",
    "bundle": "webpack --progress",
    "postbundle": "npm run test:functional",
    "precommit": "lint-staged",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:dev": "npm run deploy -- -- --env.staging",
    "deploy:staging": "npm run deploy:production",
    "deploy:production": "npm run deploy -- -- --env.production",
    "start": "npm-run-all -rp dev:test dev"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": true,
          "targets": {
            "browsers": "last 1 version, > 10%",
            "node": true,
            "exclude": [
              "typed.data-view",
              "typed.int8-array",
              "typed.uint8-array",
              "typed.uint8-clamped-array",
              "typed.int16-array",
              "typed.uint16-array",
              "typed.int32-array",
              "typed.uint32-array",
              "typed.float32-array",
              "typed.float64-array"
            ]
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "env": {
      "dev": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      },
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "jest": {
    "verbose": false,
    "moduleNameMapper": {
      "config.yml$": "<rootDir>/src/__mocks__/configMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/.*.js"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text-summary"
    ],
    "coverageDirectory": "reports/jest-coverage",
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es)"
    ]
  },
  "lint-staged": {
    "*.+(js|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js"
    ],
    "concurrentFiles": 4,
    "threshold": 50,
    "type": [
      "text",
      "html"
    ],
    "outputDir": "reports/flow-coverage"
  },
  "lighthouse": {
    "threshold": 20
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "1.1.0",
    "EBI-Icon-fonts": "1.2.0",
    "babel-polyfill": "6.26.0",
    "clanviewer": "0.1.8",
    "color-hash": "1.0.3",
    "core-js": "2.5.3",
    "d3": "4.13.0",
    "data-loader": "0.3.0",
    "dompurify": "^1.0.3",
    "draft-js": "0.10.5",
    "ebi-framework": "1.2.0",
    "filesize": "3.6.0",
    "foundation-sites": "^6.4.3",
    "gsap": "1.20.3",
    "history": "4.7.2",
    "idb": "2.0.4",
    "interpro-components": "^0.3.4",
    "isomorphic-fetch": "2.2.1",
    "lodash-es": "4.17.5",
    "pdb-web-components": "^0.2.0",
    "popper.js": "1.12.9",
    "prop-types": "15.6.0",
    "protvista-interpro-track": "0.0.6",
    "protvista-manager": "0.0.5",
    "protvista-navigation": "0.0.4",
    "protvista-sequence": "0.0.4",
    "protvista-track": "0.0.6",
    "protvista-zoomable": "0.0.3",
    "react": "16.2.0",
    "react-addons-transition-group": "15.6.2",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-tippy": "^1.2.2",
    "redux": "3.7.2",
    "reselect": "3.0.1",
    "taxonomy-visualisation": "0.1.6",
    "timeago.js": "^3.0.2",
    "timing-functions": "0.2.2",
    "url": "0.11.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chalk": "2.3.1",
    "chrome-launcher": "0.10.2",
    "classnames": "2.2.5",
    "compression-webpack-plugin": "1.1.6",
    "css-loader": "0.28.9",
    "empty": "0.10.1",
    "eslint": "4.17.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "1.1.6",
    "flow-bin": "0.65.0",
    "flow-coverage-report": "0.4.1",
    "glob": "7.1.2",
    "glob-loader": "0.3.0",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "iltorb": "2.0.3",
    "img-loader": "2.0.1",
    "jest": "22.3.0",
    "js-yaml": "3.10.0",
    "lighthouse": "2.9.1",
    "lint-staged": "6.1.0",
    "mkdirp": "0.5.1",
    "node-sass": "4.7.2",
    "node-zopfli": "2.0.2",
    "npm-run-all": "4.1.2",
    "offline-plugin": "4.9.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "prettier": "1.10.2",
    "puppeteer": "1.0.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "source-map": "0.7.1",
    "style-loader": "0.20.1",
    "uglifyjs-webpack-plugin": "1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dashboard": "1.1.1",
    "webpack-dev-server": "2.11.1",
    "why-did-you-update": "0.1.0",
    "worker-loader": "1.1.0",
    "yaml-loader": "0.5.0"
  }
}
