{
  "name": "new_interpro_webviews",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/new_interpro_webviews.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:flow": "flow check",
    "test:lint": "esw --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" \"./**/*.js\"",
    "test:unit": "mocha-webpack --webpack-config webpack.test.config.js --growl --reporter dot \"src/**/*test?(s).js\"",
    "test:functional": "echo \"Warning, no functional test specified yet, passing\" && exit 0",
    "dev": "webpack-dev-server",
    "dev:test": "npm-run-all -rp dev:test:*",
    "dev:test:lint": "npm run test:lint -- --watch",
    "dev:test:unit": "npm run test:unit -- --watch",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "bundle": "webpack --progress",
    "precommit": "lint-staged",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:staging": "npm run deploy -- -- --env.staging",
    "deploy:production": "npm run deploy -- -- --env.production",
    "start": "npm-run-all -rp dev:test dev",
    "stats": "webpack --env.production --profile --json > dist/stats.json"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": true,
          "targets": {
            "browsers": "last 1 version, > 10%",
            "node": true,
            "exclude": [
              "typed.data-view",
              "typed.int8-array",
              "typed.uint8-array",
              "typed.uint8-clamped-array",
              "typed.int16-array",
              "typed.uint16-array",
              "typed.int32-array",
              "typed.uint32-array",
              "typed.float32-array",
              "typed.float64-array"
            ]
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-react-constant-elements",
      "transform-react-jsx-source"
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ],
        "comments": false
      }
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.js": [
        "prettier --single-quote --trailing-comma es5 --write",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "prettier --single-quote --trailing-comma all --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.3",
    "babel-polyfill": "^6.23.0",
    "color-hash": "^1.0.3",
    "core-js": "^2.4.1",
    "d3": "^4.10.0",
    "data-loader": "^0.2.0",
    "draft-js": "^0.10.1",
    "foundation-sites": "~6.2.4",
    "gsap": "^1.20.2",
    "history": "^4.6.3",
    "idb": "^2.0.3",
    "interpro-components": "^0.3.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.4",
    "pdb-web-components": "^0.2.0",
    "popper.js": "^1.11.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "timeago.js": "^3.0.2",
    "timing-functions": "^0.1.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "empty": "^0.10.1",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.51.1",
    "flow-typed": "^2.1.5",
    "glob": "^7.1.2",
    "glob-loader": "^0.3.0",
    "html-webpack-plugin": "^2.30.0",
    "husky": "^0.14.3",
    "img-loader": "^2.0.0",
    "jsx-chai": "^4.0.0",
    "lint-staged": "^4.0.2",
    "mocha": "^3.4.2",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "source-map": "^0.5.6",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.27",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1",
    "why-did-you-update": "0.0.8",
    "yaml-loader": "^0.5.0"
  },
  "false": {}
}
