{
  "name": "interpro7-client",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/interpro7-client.git"
  },
  "engines": {
    "node": ">=8"
  },
  "engineStrict": true,
  "license": "GPL-3.0",
  "scripts": {
    "preanalyze": "npm run report:stats",
    "analyze": "npx webpack-bundle-analyzer reports/webpack-stats/stats.json",
    "test": "npm-run-all test:flow test:lint test:unit",
    "test:flow": "flow focus-check --merge-timeout=1000 .",
    "test:lint": "eslint --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" --ignore-pattern \"snippets\" \"./**/*.js\"",
    "test:unit": "jest --notify --maxWorkers=2 src",
    "test:functional": "[[ -n $IS_EBI_CLUSTER ]] && echo 'skipped functionnal test on EBI cluster' || jest --notify tests",
    "report": "npm-run-all -s --continue-on-error clean:report report:**",
    "report:build": "node scripts/report-build-info.js",
    "report:coverage:flow": "flow-coverage-report",
    "report:coverage:jest": "npm run test:unit -- --coverage",
    "report:lighthouse": "node scripts/lighthouse.js",
    "report:stats": "#mkdirp reports/webpack-stats && webpack --profile --json > reports/webpack-stats/stats.json",
    "coveralls": "cat reports/jest-coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --mode=development",
    "flow-typed:install": "npx flow-typed install",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "clean:report": "rimraf reports && mkdirp reports",
    "bundle": "webpack --progress",
    "postbundle": "npm run test:functional",
    "precommit": "lint-staged",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:dev": "npm run deploy -- -- --mode=development",
    "deploy:staging": "npm run deploy -- -- --env.staging",
    "deploy:production": "npm run deploy",
    "start": "npm run dev",
    "warmup": "node scripts/website-warmer.js"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "lint-staged": {
    "*.+(js|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "flow-coverage-report": {
    "globIncludePatterns": [
      "src/**/*.js"
    ],
    "concurrentFiles": 4,
    "threshold": 50,
    "reportTypes": [
      "text",
      "html",
      "json"
    ],
    "outputDir": "reports/flow-coverage"
  },
  "lighthouse": {
    "threshold": 20
  },
  "dependencies": {
