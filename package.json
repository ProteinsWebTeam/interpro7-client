{
  "name": "interpro7-client",
  "version": "0.0.6",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProteinsWebTeam/interpro7-client.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:flow": "flow check",
    "test:lint": "esw --color --ignore-path .gitignore --ignore-pattern \"flow-typed\" \"./**/*.js\"",
    "test:unit": "jest --notify",
    "test:functional": "echo \"Warning, no functional test specified yet, passing\" && exit 0",
    "dev": "webpack-dev-server",
    "dev:test": "npm-run-all -rp dev:test:*",
    "dev:test:lint": "npm run test:lint -- --watch",
    "dev:test:unit": "npm run test:unit -- --watch",
    "flow-typed:install": "flow-typed install",
    "lint:fix": "npm run test:lint -- --fix",
    "clean": "rimraf dist",
    "bundle": "webpack --progress",
    "precommit": "lint-staged",
    "predeploy": "npm-run-all test clean",
    "deploy": "npm run bundle",
    "deploy:staging": "npm run deploy -- -- --env.staging",
    "deploy:production": "npm run deploy -- -- --env.production",
    "start": "npm-run-all -rp dev:test dev",
    "stats": "webpack --env.production --profile --json > dist/stats.json"
  },
  "contributors": [
    "Aur√©lien Luciani <luciani@ebi.ac.uk> (https://github.com/aurel-l/)"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true,
          "useBuiltIns": true,
          "targets": {
            "browsers": "last 1 version, > 10%",
            "node": true,
            "exclude": [
              "typed.data-view",
              "typed.int8-array",
              "typed.uint8-array",
              "typed.uint8-clamped-array",
              "typed.int16-array",
              "typed.uint16-array",
              "typed.int32-array",
              "typed.uint32-array",
              "typed.float32-array",
              "typed.float64-array"
            ]
          }
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-react-constant-elements",
      "transform-react-jsx-source"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "moduleNameMapper": {
      "config.yml$": "<rootDir>/__mocks__/configMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/.*.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "lint-staged": {
    "*.+(js|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.10",
    "EBI-Icon-fonts": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "color-hash": "^1.0.3",
    "core-js": "^2.5.1",
    "d3": "^4.10.2",
    "data-loader": "^0.2.0",
    "draft-js": "^0.10.1",
    "ebi-framework": "^1.2.0",
    "filesize": "^3.5.10",
    "foundation-sites": "^6.4.3",
    "gsap": "^1.20.2",
    "history": "^4.7.2",
    "idb": "^2.0.3",
    "interpro-components": "^0.3.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.4",
    "pdb-web-components": "^0.2.0",
    "popper.js": "^1.12.5",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^4.0.5",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "timeago.js": "^3.0.2",
    "timing-functions": "^0.1.4",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "empty": "^0.10.1",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.54.0",
    "flow-typed": "^2.1.5",
    "glob": "^7.1.2",
    "glob-loader": "^0.3.0",
    "html-webpack-plugin": "^2.30.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^2.0.0",
    "jest": "^21.0.1",
    "js-yaml": "^3.9.1",
    "lint-staged": "^4.1.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "offline-plugin": "^4.8.3",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map": "^0.5.7",
    "style-loader": "^0.18.2",
    "uglify-js": "^3.0.28",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.6.1",
    "why-did-you-update": "0.0.8",
    "worker-loader": "^0.8.1",
    "yaml-loader": "^0.5.0"
  }
}
