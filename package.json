    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|css|ts|tsx)": [
      "prettier --write"
    ]
  },
  "lighthouse": {
    "threshold": 20
  },
  "overrides": {
    "core-js": "3.35.0"
