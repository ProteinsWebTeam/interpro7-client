[ignore]
<PROJECT_ROOT>/\..*
<PROJECT_ROOT>/dist/.*
.*/node_modules/flow-typed
.*/node_modules/editions*
.*/node_modules/fbjs.*
.*/node_modules/babel.*
.*/node_modules/draft-js
.*/node_modules/.*json
.*/node_modules/eslint-plugin*
.*/node_modules/timing-functions
.*/node_modules/@most
.*/node_modules/flow-coverage-report*

[include]


[libs]
./flow-typed

[options]
module.name_mapper='^\(.*\).css$' -> 'empty/object'
module.name_mapper='^\(.*\).scss$' -> 'empty/object'
module.name_mapper='^\(.*\).yml$' -> 'empty/object'
module.name_mapper='^timing-functions/src$' -> 'empty/object'
module.name_mapper='^litemol$' -> 'empty/object'
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
munge_underscores=true
emoji=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
